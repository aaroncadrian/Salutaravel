{
    "name": "aaroncadrian/salutaravel",
    "description": "A Laravel package for managing and validating salutations such as titles and suffixes.",
    "require": {
        "php": ">=5.5",
        "illuminate/support": ">=5.0,<5.6",
        "illuminate/validation": ">=5.0,<5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Adrian",
            "email": "aaroncadrian@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AaronAdrian\\Salutaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronAdrian\\Salutaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronAdrian\\Salutaravel\\SalutaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
